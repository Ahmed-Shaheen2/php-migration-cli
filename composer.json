  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "byjg/migration": "4.9.*",
    "symfony/console": "^5.4|^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\DbMigration\\Console\\": "src/"
    }
  },
  "bin": [
    "scripts/migrate"
  ],
  "license": "MIT"
}
